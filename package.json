{
  "name": "cheng-music",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "better-scroll": "^2.0.5",
    "core-js": "^2.6.5",
    "element-ui": "^2.14.0",
    "good-storage": "^1.1.1",
    "imagvue": "0.0.5",
    "lodash-es": "^4.17.15",
    "path": "^0.12.7",
    "postcss-pxtorem": "^5.1.1",
    "vue": "^2.6.10",
    "vue-i18n": "^8.22.1",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.0.3",
    "vue-xgplayer": "^0.4.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "space-before-function-paren": [0, "always"],
      "no-debugger": 1,
      "indent": ["error", 2, {"SwitchCase": 1}],
      "quotes": ["error", "double"],
      "brace-style": ["error", "stroustrup"],
      "array-bracket-spacing": ["error", "never"],
      "semi": ["error", "always"],
      "no-console": ["off"]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 14,
        "propList": [
          "*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}